{
  "name": "tsx-tools-suite",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:widgets": "tsup widgets/register-all.ts --format iife --global-name TSXWidgets --out-dir dist/widgets --minify --sourcemap",

    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "i": "^0.3.7",
    "npm": "^11.5.2",
    "postcss": "^8.4.38",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.7",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4",
    "zod": "^3.23.8"
  }

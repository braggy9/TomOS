    "prisma:migrate": "prisma migrate dev --name init",
    "db:seed": "node prisma/seed.mjs",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "test": "vitest run --reporter dot"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@notionhq/client": "^2.2.15",
    "docx": "^8.5.0",

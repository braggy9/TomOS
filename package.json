    "lucide-react": "^0.460.0",
    "next": "^14.2.32",
    "node-cron": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-to-webcomponent": "^1.7.2",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.14.0",
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-interactions": "^8.1.0",
    "@storybook/nextjs": "^8.1.0",
    "@storybook/react": "^8.1.0",
    "@types/node": "^20.19.11",
    "@types/node-cron": "^3.0.11",
